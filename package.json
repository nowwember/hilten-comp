    "socket.io-client": "4.7.5",
    "swr": "2.2.5",
    "tailwindcss": "3.4.10",
    "unified": "11.0.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "dompurify": "3.2.6",
    "dotenv": "17.2.2",
    "jsdom": "26.1.0",
    "playwright": "1.55.0",
    "ts-node": "10.9.2",
    "tsx": "4.20.5",
    "typescript": "5.5.4"
